{
  "version": 2,
  "builds": [
    {
      "src": "businessconnect-senegal/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "businessconnect-senegal/client/build/$1"
    }
  ],
  "buildCommand": "cd businessconnect-senegal/client && npm install && npm run build",
  "outputDirectory": "businessconnect-senegal/client/build",
  "installCommand": "cd businessconnect-senegal/client && npm install"
} 